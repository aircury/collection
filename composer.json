{
    "name": "aircury/collection",
    "description": "Library to provide collection structures, to use instead of arrays",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":  "Jose Diaz",
            "email": "jose@aircury.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/aircury/code-manager.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aircury\\Collection\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aircury\\Collection\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "clue/graph": "^0.9.3",
        "graphp/algorithms": "^0.8.2"
    },
    "require-dev": {
        "aircury/code-manager": "dev-main",
        "phpspec/prophecy-phpunit": "^2.2",
        "phpunit/phpunit": "^11.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin",
        "discard-changes": true
    }
}
